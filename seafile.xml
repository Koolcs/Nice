<?xml version="1.0"?>
<Container>
  <Name>seafile</Name>
  <Repository>docker.seafile.top/seafileltd/seafile-pro:latest</Repository>
  <Registry></Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support/>
  <Project/>
  <Overview>seafile</Overview>
  <Category/>
  <WebUI>http://[IP]:[PORT:8001]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/Koolcs/Nice/master/image/seafile-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8001</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>18080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>1443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/data/seafile-pro</HostDir>
      <ContainerDir>/shared</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>your mail</Value>
      <Name>SEAFILE_ADMIN_EMAIL</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>your password</Value>
      <Name>SEAFILE_ADMIN_PASSWORD</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>IP OR domain:PORT</Value>
      <Name>SEAFILE_SERVER_HOSTNAME</Name>
      <Mode/>
    </Variable>
	<Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>
