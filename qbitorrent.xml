<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://github.com/Koolcs/Nice/blob/master/qbitorrent.xml</TemplateURL>
  <Beta>False</Beta>
  <Category></Category>
  <Name>qbitorrent</Name>
  <Description>
  </Description>
  <Overview>
qbitorrent
  </Overview>
  <Support>https://github.com/Koolcs/Nice</Support>
  <Registry>https://hub.docker.com/r/linuxserver/qbittorrent/</Registry>
  <GitHub>https://github.com/Koolcs/Nice</GitHub>
  <Repository>linuxserver/qbittorrent</Repository>
  <Project>https://www.qbittorrent.org/</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>6881</HostPort>
        <ContainerPort>6881</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>6881</HostPort>
        <ContainerPort>6881</ContainerPort>
        <Protocol>udp</Protocol>
      </Port>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>	  
    </Publish>
    </Networking>
  <Environment>
    <Variable>
      <Name>PUID</Name>
      <Value>99</Value>
    </Variable>
    <Variable>
      <Name>PGID</Name>
      <Value>100</Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/qbittorrent</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/data/qbittorrent/downloads</HostDir>
      <ContainerDir>/downloads</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <Icon>https://github.com/Koolcs/Nice/blob/master/image/qbittorrent-icon.png</Icon>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <DonateImg></DonateImg>
  <ExtraParams></ExtraParams>
</Containers>
