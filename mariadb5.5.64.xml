<?xml version="1.0"?>
<Container>
  <Name>mariadb</Name>
  <Repository>mariadb:5.5.64</Repository>
  <Registry>https://hub.docker.com/r/mariadb:5.5.64/</Registry>
  <Network>bridge</Network>
  <Privileged>false</Privileged>
  <Support>https://github.com/Koolcs/Nice</Support>
  <Project>https://mariadb.org/</Project>
  <Overview> mariadb 5.5.64 </Overview>
  <Category>Network:Other MediaApp:Other Other: Tools:Utilities</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/mariadb.xml</TemplateURL>
  <Icon>https://d1q6f0aelx0por.cloudfront.net/product-logos/library-mariadb-logo.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled></DateInstalled>
  <DonateText></DonateText>
  <DonateLink></DonateLink>
  <Description> mariadb 5.5.64 </Description>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>3306</HostPort>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/mariadb</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir>/mnt/user/data/mysql/data</HostDir>
      <ContainerDir>/var/lib/mysql</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
	<Volume>
      <HostDir>/mnt/user/data/mysql/mariadb.cnf</HostDir>
      <ContainerDir>/etc/mysql/conf.d/mariadb.cnf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>99</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>100</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>password</Value>
      <Name>MYSQL_ROOT_PASSWORD</Name>
      <Mode/>
    </Variable>
  </Environment>
</Container>
